<?xml version='1.0' encoding='utf-8'?>
<odoo>

        <!-- Actions -->

        <record id="website_leaflet_mapfeature_action" model="ir.actions.act_window">
            <field name="name">Map Features</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">website_leaflet.mapfeature</field>
            <field name="view_mode">kanban,tree,form,geoengine</field>
            <field name="view_type">form</field>
<!--            <field name="view_id" ref="website_leaflet_mapfeature_view_form"/>-->
        </record>

        <record id="website_leaflet_map_action" model="ir.actions.act_window">
            <field name="name">Maps</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">website_leaflet.map</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_type">form</field>
<!--            <field name="view_id" ref="website_leaflet_map_view_form"/>-->
        </record>

        <record id="website_leaflet_category_action" model="ir.actions.act_window">
            <field name="name">Categories</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">website_leaflet.category</field>
            <field name="view_mode">kanban,tree,form</field>
            <field name="view_type">form</field>
<!--            <field name="view_id" ref="website_leaflet_category_view_form"/>-->
        </record>

        <!-- Menus -->
        <menuitem id="website_leaflet_main_menu" name="Leaflet" sequence="17"
            web_icon="website_leaflet,static/description/icon.png"/>

        <menuitem id="geoengine_category_menu" name="Categories" parent="website_leaflet_main_menu"
            action="website_leaflet_category_action" sequence="1"/>

        <menuitem id="website_leaflet_map_feature_menu" name="MapFeature"
            parent="website_leaflet_main_menu" action="website_leaflet_mapfeature_action"
            sequence="2" groups="base_geoengine.group_geoengine_user,base_geoengine.group_geoengine_admin"/>

        <menuitem id="geoengine_map_menu" name="Maps" parent="website_leaflet_main_menu"
            action="website_leaflet_map_action" sequence="3"/>

</odoo>
